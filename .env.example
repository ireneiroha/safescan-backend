# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration (Local Development)
# Use these for local development with PostgreSQL
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=your_local_password
PGDATABASE=safescan

# OR use DATABASE_URL (works for both local and production)
# DATABASE_URL=postgres://user:password@localhost:5432/safescan

# Authentication
JWT_SECRET=your_jwt_secret_here
CORS_ORIGIN=http://localhost:3000

# Google Sheets (optional)
# GOOGLE_SHEETS_ID=your_google_sheets_id
